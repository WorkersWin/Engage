<h1>Assessment Levels to Date</h1>
<table class="table table-hover">
<thead>
<tr><th>Level</th><th>Count</th></tr>
</thead>
<tbody>
<% @assessments.sort.each do |level| %>
  <tr><td><%= level[1] %></td><td><%= level[2] %></td></tr>
<% end %>
</tbody>
<tfoot>
  <tr><td>Total</td><td><%= @total %></td></tr>
</tfoot>
</table>
