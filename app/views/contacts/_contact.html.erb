<div id="<%= dom_id contact %>">
  <p>
    <strong>First name:</strong>
    <%= contact.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= contact.last_name %>
  </p>

  <p>
    <strong>Pronouns:</strong>
    <%= contact.pronouns %>
  </p>

  <p>
    <strong>Mobile phone:</strong>
    <%= contact.mobile_phone %>
  </p>

  <p>
    <strong>Personal email:</strong>
    <%= contact.personal_email %>
  </p>

</div>
